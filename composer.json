{
    "name": "rideon/delivery-app",
    "description": "Tracking delivery application.",
    "keywords": [
        "delivery", 
        "tracking",
        "merchants",
        "drivers",
        "customers"
    ],
    "license": "Private",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "barryvdh/laravel-snappy": "^0.4.6",
        "braintree/braintree_php": "^4.5",
        "brozot/laravel-fcm": "^1.2",
        "facebook/graph-sdk": "*",
        "florianv/laravel-swap": "^2.0",
        "google/apiclient": "^2.2",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.6",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "intervention/image": "dev-master",
        "laravel/framework": "5.8.38",
        "laravel/socialite": "*",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "omnipay/paypal": "^3.0",
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/message": "^1.7",
        "shanmuga/laravel-entrust": "^1.0",
        "stripe/stripe-php": "^7.12",
        "tymon/jwt-auth": "dev-develop",
        "web-token/jwt-framework": "^2.1",
        "yajra/laravel-datatables": "^1.5",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.6",
        "facade/ignition": "^1.6",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "vendor/google/apiclient/src/Google"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/Start/helpers.php",
            "app/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
